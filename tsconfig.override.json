{
    "compilerOptions": {
      "strict": false,
      "baseUrl": "./src",
      "allowJs": true,
      "noEmitOnError": true,
      "paths": {
        "~/*": ["./*"],
        "@api/*": ["./api/*"],
        "@assets/*": ["./assets/*"],
        "@store":["./redux/store.ts"],
        "@redux/*": ["./redux/*"],
        "@components/*": ["./components/*"],
        "@utils/*": ["./utils/*"]
      }
  }
}